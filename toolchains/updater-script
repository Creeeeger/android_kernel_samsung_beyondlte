show_progress(0.07, 0);

# Set progress to 60%
set_progress(0.6);

# Inform the user about the flashing process
ui_print("Flashing boot, dtb, and dtbo images...");

# Extract and flash boot image
package_extract_file("boot.img", "/dev/block/platform/13d60000.ufs/by-name/boot");
set_progress(0.7);

# Extract and flash dtb image
package_extract_file("dtb.img", "/dev/block/platform/13d60000.ufs/by-name/dtb");
set_progress(0.8);

# Extract and flash dtbo image
package_extract_file("dtbo.img", "/dev/block/platform/13d60000.ufs/by-name/dtbo");
set_progress(0.9);

# Inform the user about the completion of flashing
ui_print("\nDone flashing packages\n");

# Set progress to 100%
set_progress(1.0);

